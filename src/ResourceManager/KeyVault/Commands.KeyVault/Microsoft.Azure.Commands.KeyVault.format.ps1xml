      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.KeyVault.Models.KeyBundle</TypeName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>VaultName</Label>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>KeyName</Label>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>KeyType</Label>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Enabled</Label>
            <Alignment>Left</Alignment>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Attributes</Label>
            <Alignment>Left</Alignment>
          </TableColumnHeader>          
		  <TableColumnHeader>
            <Label>Key</Label>
            <Alignment>Left</Alignment>
          </TableColumnHeader>          
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>VaultName</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>KeyName</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.Attributes.KeyType</ScriptBlock>                 
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.Attributes.Enabled</ScriptBlock>                
              </TableColumnItem>
              <TableColumnItem>
                <PropertyName>Attributes</PropertyName>
