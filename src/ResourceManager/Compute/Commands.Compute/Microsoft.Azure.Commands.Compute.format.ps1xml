<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  <ViewDefinitions>

    <View>
      <Name>Microsoft.WindowsAzure.Management.Srp.Models.StorageAccount</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.WindowsAzure.Management.Srp.Models.StorageAccount</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>AccountType</Label>
                <ScriptBlock>$_.Properties.AccountType</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>Location</Label>
                <PropertyName>Location</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Tags</Label>
                <PropertyName>Tags</PropertyName>
              </ListItem>
              <ListItem>
                <Label>CreationTime</Label>
                <ScriptBlock>$_.Properties.CreationTime</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>CustomDomains</Label>
                <ScriptBlock>$_.Properties.CustomDomains</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>LastGeoFailoverTime</Label>
                <ScriptBlock>$_.Properties.LastGeoFailoverTime</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>PrimaryEndpoints</Label>
                <ScriptBlock>$_.Properties.PrimaryEndpoints</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>PrimaryLocation</Label>
                <ScriptBlock>$_.Properties.PrimaryLocation</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ProvisioningState</Label>
                <ScriptBlock>$_.Properties.ProvisioningState</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>SecondaryEndpoints</Label>
                <ScriptBlock>$_.Properties.SecondaryEndpoints</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>SecondaryLocation</Label>
                <ScriptBlock>$_.Properties.SecondaryLocation</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>StatusOfPrimary</Label>
                <ScriptBlock>$_.Properties.StatusOfPrimary</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>StatusOfSecondary</Label>
                <ScriptBlock>$_.Properties.StatusOfSecondary</ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

    <View>
      <Name>Microsoft.WindowsAzure.Management.Srp.Models.StorageAccountOutput</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.WindowsAzure.Management.Srp.Models.StorageAccountOutput</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>AccountType</Label>
                <ScriptBlock>$_.Properties.AccountType</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>Location</Label>
                <PropertyName>Location</PropertyName>
              </ListItem>
              <ListItem>
                <Label>LocationUrl</Label>
                <PropertyName>LocationUrl</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Tags</Label>
                <PropertyName>Tags</PropertyName>
              </ListItem>
              <ListItem>
                <Label>AzureAsyncOperation</Label>
                <PropertyName>AzureAsyncOperation</PropertyName>
              </ListItem>
              <ListItem>
                <Label>CreationTime</Label>
                <ScriptBlock>$_.Properties.CreationTime</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>CustomDomains</Label>
                <ScriptBlock>$_.Properties.CustomDomains</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>LastGeoFailoverTime</Label>
                <ScriptBlock>$_.Properties.LastGeoFailoverTime</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>PrimaryEndpoints</Label>
                <ScriptBlock>$_.Properties.PrimaryEndpoints</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>PrimaryLocation</Label>
                <ScriptBlock>$_.Properties.PrimaryLocation</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>ProvisioningState</Label>
                <ScriptBlock>$_.Properties.ProvisioningState</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>SecondaryEndpoints</Label>
                <ScriptBlock>$_.Properties.SecondaryEndpoints</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>SecondaryLocation</Label>
                <ScriptBlock>$_.Properties.SecondaryLocation</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>StatusOfPrimary</Label>
                <ScriptBlock>$_.Properties.StatusOfPrimary</ScriptBlock>
            </ListItem>
              <ListItem>
                <Label>StatusOfSecondary</Label>
                <ScriptBlock>$_.Properties.StatusOfSecondary</ScriptBlock>
            </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

    <View>
      <Name>Microsoft.Azure.Commands.Compute.Models.VirtualMachine</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Compute.Models.VirtualMachine</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Location</Label>
                <PropertyName>Location</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Tags</Label>
                <PropertyName>Tags</PropertyName>
              </ListItem>
              <ListItem>
                <Label>TagsValue</Label>
                <PropertyName>TagsValue</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Type</Label>
                <PropertyName>Type</PropertyName>
              </ListItem>
              <ListItem>
                <Label>ProvisioningState</Label>
                <ScriptBlock>
                  $_.VMProfile.ProvisioningState
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>VMProfile</Label>
                <PropertyName>VMProfile</PropertyName>
              </ListItem>
              <ListItem>
                <Label>VirtualMachineSize</Label>
                <ScriptBlock>
                  $_.VMProfile.HardwareProfile.VirtualMachineSize
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>NetworkProfile</Label>
                <ScriptBlock>
                  foreach ($nic in $_.VMProfile.NetworkProfile.NetworkInterfaces)
                  {
                  "NetworkInterfaceName : " + $nic.Name;
                  "`n IPConfigurations     : " + $nic.Properties.IPConfigurations.Count + " configuration(s)";
                  foreach ($ip in $nic.Properties.IPConfigurations)
                  {
                  "`n  [PublicIPName       : " + $ip.Name;
                  "`n   PublicIPAddressUri : " + $ip.PublicIPAddress.ReferenceUri + "]";
                  }
                  }
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>AdminUsername</Label>
                <ScriptBlock>
                  $_.VMProfile.OSProfile.AdminUsername
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ComputerName</Label>
                <ScriptBlock>
                  $_.VMProfile.OSProfile.ComputerName
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>DestinationVhdsContainer</Label>
                <ScriptBlock>
                  $_.VMProfile.StorageProfile.DestinationVhdsContainer
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>SourceImage</Label>
                <ScriptBlock>
                  $_.VMProfile.StorageProfile.SourceImage.ReferenceUri
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>OSDisk</Label>
                <ScriptBlock>
                  "OSDiskName    : " + $_.VMProfile.StorageProfile.OSDisk.Name;
                  "`n OSDiskUri     : " + $_.VMProfile.StorageProfile.OSDisk.VhdUri;
                  "`n OSDiskCaching : " + $_.VMProfile.StorageProfile.OSDisk.Caching;
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>DataDisks</Label>
                <ScriptBlock>
                  foreach ($disk in $_.VMProfile.StorageProfile.DataDisks)
                  {
                  "DiskSizeGB  : " + $disk.DiskSizeGB;
                  "`n LUN         : " + $disk.Lun;
                  "`n Caching     : " + $disk.Caching;
                  "`n DiskName    : " + $disk.Name;
                  "`n VhdUri      : " + $disk.VhdUri
                  }
                </ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
    <View>
      <Name>Microsoft.Azure.Commands.Compute.Models.PSVirtualMachineProfile</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Compute.Models.PSVirtualMachineProfile</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>HardwareProfile</Label>
                <PropertyName>HardwareProfile</PropertyName>
              </ListItem>
              <ListItem>
                <Label>NetworkProfile</Label>
                <PropertyName>NetworkProfile</PropertyName>
              </ListItem>
              <ListItem>
                <Label>OSProfile</Label>
                <PropertyName>OSProfile</PropertyName>
              </ListItem>
              <ListItem>
                <Label>StorageProfile</Label>
                <PropertyName>StorageProfile</PropertyName>
              </ListItem>
              <ListItem>
                <Label>ProvisioningState</Label>
                <PropertyName>ProvisioningState</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
  </ViewDefinitions>
</Configuration>
