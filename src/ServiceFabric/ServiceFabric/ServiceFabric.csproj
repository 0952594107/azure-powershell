<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PsModuleName>ServiceFabric</PsModuleName>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)..\..\Az.props" />

  <PropertyGroup>
    <RootNamespace>$(LegacyAssemblyPrefix)$(PsModuleName)</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.KeyVault" Version="3.0.1" />
    <PackageReference Include="Microsoft.Azure.KeyVault.WebKey" Version="3.0.1" />
    <PackageReference Include="Microsoft.Azure.Management.ServiceFabric" Version="1.1.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Template\Linux\parameter.json" CopyToOutputDirectory="PreserveNewest" />
    <None Update="Template\Linux\template.json" CopyToOutputDirectory="PreserveNewest" />
    <None Update="Template\Windows\parameter.json" CopyToOutputDirectory="PreserveNewest" />
    <None Update="Template\Windows\template.json" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>

</Project>